{
  "name": "fedlearn-industrial",
  "version": "1.0.0",
  "description": "Federated Learning Platform for Industrial IoT & Automation",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "init": "./init.sh"
  },
  "keywords": [
    "federated-learning",
    "industrial-iot",
    "machine-learning",
    "edge-computing"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
